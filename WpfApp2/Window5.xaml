                </Calendar.BlackoutDates>
            </Calendar>
            

        </StackPanel>
        <TextBox Height="69" TextWrapping="Wrap" Width="140" Margin="55,315,605,50" x:Name="TB1"/>
        <TextBox Height="69" TextWrapping="Wrap" Width="140" Margin="612,315,48,50" x:Name="TB2"/>
    </Grid>
